#N4 ���������� ����� � ��������� ���������;
print("\nN4 ���������� ����� � ��������� ���������")
unique_letters,x,z=[],[],0
for i in marks:
    for j in i:
        for k in x:
            if j == k:
                z+=1
        if z==0:
            x.append(j)
        z=0
for i in x:
    for j in marks:
        for k in j:
            if i == k:
                z+=1
    if z==1:
         unique_letters.append(i)
    z=0        
print(*unique_letters)

