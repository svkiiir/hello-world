n = int(input("������� �����, �� �������� �� ����� ������� ������������������: "))
def alikvot(n):
    print(n,end=', ')
    a = 0
    for i in range(1,n):
        if n%i == 0:
            a += i
    if a != 0:
        return alikvot(a)
    else: 
        print('0')
alikvot(n)
