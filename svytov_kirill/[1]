#N6 ��������������� �� �������� (� �������� �������) ������ �������������;
print("\nN6 ��������������� �� �������� (� �������� �������) ������ �������������")
def quicksort(nums, f, l):
    if f >= l: 
        return
    i, j = f, l
    p = nums[(l + f) // 2]
    while i <= j:
        while nums[i] > p: i += 1
        while nums[j] < p: j -= 1
        if i <= j:
            nums[i], nums[j] = nums[j], nums[i]
            i, j = i + 1, j - 1
        quicksort(nums, f, j)
        quicksort(nums, i, l)
unique_names.remove("������")
quicksort(unique_names, 0, len(unique_names)-1)
print(*unique_names)

