#N8 FIFO �������
print("\nN8 FIFO �������")
import queue
q = queue.Queue(100)
a = int(input("����� ����� �� �������, �������: '0'\n"))
while a!=0:
    q.put(a)
    a = int(input())
print("���������� �������: ",end='')
while not q.empty():
    print(q.get(),end=' ')
print()

